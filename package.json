{
  "name": "vitestory",
  "version": "0.0.0",
  "type": "module",
  "exports": {
    ".": {
      "default": "./dist/index.js",
      "module": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./modules": {
      "default": "./dist/modules/index.js",
      "module": "./dist/modules/index.js",
      "types": "./dist/modules/index.d.ts"
    }
  },
  "workspaces": [
    "apps/*"
  ],
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "vite build",
    "build:watch": "vite build --watch",
    "docs:dev": "npm run dev -w apps/docs"
  },
  "dependencies": {
    "vue": "^3.5.12"
  },
  "devDependencies": {
    "@types/node": "^22.8.6",
    "@vitejs/plugin-vue": "^5.1.4",
    "typescript": "~5.6.2",
    "vite": "^5.4.10",
    "vite-plugin-dts": "^4.3.0",
    "vue-tsc": "^2.1.8"
  }
}
